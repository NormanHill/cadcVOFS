<project>

<!-- install jars and wars -->
<target name="run" depends="build">
	<copy overwrite="true" todir="${user.home}/work/test/webapps">
		<fileset dir="${build}/webapps" />
	</copy>
	<copy overwrite="true" todir="${user.home}/work/lib">
		<fileset dir="${build}/lib">
			<exclude name="**.trunk.**" />
		</fileset>
	</copy>
</target>

<target name="checkDeploy">
	<get src="http://localhost/${webapp}/logControl" dest="/dev/null" />
</target>

</project>
